package Generics;

public class cgenerics_one_lion<T extends number> {
	
	// public class cgenerics_one_lion<T extends number> we can also extend like this in class
	// In place of " T " we can write any alphabet, mostly all members will write T only
	T a;
	
	public void printmethod()
	{
		System.out.println(a);
	}

}
