package mConverstionFromPOJOtoJSONandJSONtoPOJO;

import io.restassured.RestAssured;

public class gConvertSimpleJSONArrayResponsetoPOJO {

	public static void main(String[] args) {

		Address[] addresses = 
				RestAssured
					.get("https://run.mocky.io/v3/0b94e925-8780-4efa-9fb6-0645561fa41c")
					.as(Address[].class);
		
		System.out.println("No of Addresses :" +addresses.length);
		System.out.println(addresses[0].);
		
		
		
	}

}
