public class bPOJOCompanyHiringEmployees {

	public static void main(String[] args) {

		aEmployeePojoExample shaik = new aEmployeePojoExample();
		shaik.id = 10;
		shaik.name = "abdul";
		shaik.mobno = "123456789";
		shaik.gender = "male";
		shaik.departid = 100;
		shaik.address ="gnt";
		
		aEmployeePojoExample abdul = new aEmployeePojoExample();
		abdul.id = 20;
		abdul.name = "sshaik";
		abdul.mobno = "1234567890";
		abdul.gender = "male";
		abdul.departid = 101;
		abdul.address ="gnt1";
		
		
	}

}
