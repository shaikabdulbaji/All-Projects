package json;

import io.restassured.path.json.JsonPath;

public class eJsonPathforSimpleJsonArray {

	public static void main(String[] args) {

		String jsonArray = "[\r\n"
				+ "  \"10\",\r\n"
				+ "  \"20\",\r\n"
				+ "  \"30\",\r\n"
				+ "  \"40\",\r\n"
				+ "  \"50\"\r\n"
				+ "]";
		
		JsonPath jp = new JsonPath(jsonArray);
		
		System.out.println(jp.get("[0]"));
	
	
	
	}

}
