package OtherConceptsOfRestAssured;

import org.testng.annotations.Test;

import io.restassured.RestAssured;

public class HeadersExample {
	
	
	@Test
	public void passHeader()
	{
		
		RestAssured
				.given()
				.log()
				.all()
				.headers("header1", "value")
				.when()
				.get();
		
		
		
	}

}
